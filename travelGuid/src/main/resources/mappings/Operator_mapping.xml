<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="OperatorDAO">
	<select id="getOperator" resultType="operator">
		SELECT 
			*
		FROM 
			operator
		WHERE
			o_id = #{o_id}
		AND
			o_pwd = #{o_pwd}
	</select>
</mapper>